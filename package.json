{
  "name": "ax1-services",
  "version": "1.0.0",
  "scripts": {
    "build": "tsc",
    "deploy": "serverless deploy",
    "deploy:dev": "serverless deploy --stage dev",
    "deploy:prod": "serverless deploy --stage prod",
    "deploy:dian": "cd lambdas/dian && serverless deploy",
    "deploy:dian:dev": "cd lambdas/dian && serverless deploy --stage dev",
    "deploy:dian:prod": "cd lambdas/dian && serverless deploy --stage prod",
    "remove": "serverless remove",
    "remove:dian": "cd lambdas/dian && serverless remove",
    "invoke:dian:auth": "cd lambdas/dian && serverless invoke local -f auth -p ../../events/dian-auth.json"
  },
  "devDependencies": {
    "@serverless/compose": "^1.3.0",
    "@types/aws-lambda": "^8.10.130",
    "@types/node": "^20.10.0",
    "serverless": "^3.38.0",
    "serverless-plugin-typescript": "^2.1.5",
    "typescript": "^5.3.2"
  }
}
